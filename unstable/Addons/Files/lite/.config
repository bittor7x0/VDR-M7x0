CONFIG_GENERATE_ROOTFS_DIR = y
# CONFIG_ROOTFS_DIR is not set
# CONFIG_GENERATE_EXT2_ROOTFS_IMAGE is no set
# CONFIG_EXT2_ROOTFS_SIZE is not set
